    "import numpy as np\n",
    "a=np.array([[1.0,2.0],[3.0,4.0]])\n",
    "print(a)\n",
    "print(a.transpose())\n",
    "np.linalg.inv(a)\n",
    "u=np.eye(3)\n",
    "print(u)\n",
    "j = np.array([[0.0, -1.0], [1.0, 0.0]])\n",
    "j@j              #matrix product\n",
    "np.trace(a)\n",
    "y=np.array([[5.],[7.]])\n",
    "np.linalg.solve(a,y)\n",
    "np.linalg.eig(j)"
   ]
  }
